<manifest xmlns:android="http://schemas.android.com/apk/res/android"

          package="com.lenovohit.module_queue"
>

    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:supportsRtl="true"
    >
    <service android:name=".exposure.QueueRouterConnectService"
             android:process=":module_queue"/>
        <activity android:name=".ui.LX_QueueUpActivity"
                  android:configChanges="orientation|keyboardHidden"
                  android:screenOrientation="portrait"
                  android:launchMode="singleTask"
                  android:process=":module_queue"/>
        <activity android:name=".ui.LX_QueueUpDetailActivity"
                  android:configChanges="orientation|keyboardHidden"
                  android:screenOrientation="portrait"
                  android:launchMode="singleTask"
                  android:process=":module_queue"/>
    </application>

</manifest>
